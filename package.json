{
   "devDependencies": {
      "@types/express": "^4.17.21",
      "@types/node": "^16.11.10",
      "@types/sqlite3": "^3.1.11",
      "express": "^4.19.2",
      "install": "^0.13.0",
      "npm": "^10.8.3",
      "sqlite3": "^5.1.7",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   },
   "dependencies": {
      "dotenv": "^16.4.5",
      "nodemon": "^3.1.4",
      "reflect-metadata": "^0.1.13",
      "ts-node-dev": "^2.0.0",
      "typeorm": "0.3.20",
      "pg": "^8.4.0"
   },
   "scripts": {
      "start": "ts-node-dev src/index.ts",
      "build": "tsc",
      "serve": "node dist/index.js",
      "crawl": "ts-node src/crawler/crawler.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "typeorm:generate": "npm run typeorm migration:generate -- -d ./src/data/appDataSource.ts ./src/data/migrations/addedInitiativeEntities",
      "typeorm:migrate": "npx typeorm-ts-node-esm migration:run -d src/data/appDataSource.ts",
      "typeorm:revert": "npx typeorm-ts-node-esm migration:revert -d src/data/appDataSource.ts",
      "typeorm:drop": "npx typeorm-ts-node-esm schema:drop -d src/data/appDataSource.ts"
   }
}